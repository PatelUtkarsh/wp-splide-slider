{
  "$schema": "https://json.schemastore.org/block.json",
  "apiVersion": 1,
  "name": "splide/carousel",
  "version": "0.1.0",
  "title": "splide Carousel",
  "category": "media",
  "icon": "slides",
  "description": "Example block written with ESNext standard and JSX support â€“ build step required.",
  "textdomain": "splide-carousel",
  "editorScript": "file:./../../build/carousel.js",
  "editorStyle": "file:./../../build/carousel.css",
  "viewScript": "file:./../../build/view.js",
  "supports": {
	"align": [
	  "wide",
	  "full"
	]
  },
  "attributes": {
	"type": {
	  "type": "string",
	  "default": "slide",
	  "options": [
		"slide",
		"loop",
		"fade"
	  ]
	},
	"direction": {
	  "type": "string",
	  "default": "ltr",
	  "options": [
		"ltr",
		"rtl",
		"ttb"
	  ]
	},
	"perPage": {
	  "type": "number",
	  "default": 1
	},
	"width": {
	  "type": "string",
	  "default": "100%"
	},
	"rewind": {
	  "type": "boolean",
	  "default": false
	},
	"speed": {
	  "type": "number",
	  "default": 1000
	},
	"wheel": {
	  "type": "boolean",
	  "default": false
	}
  }
}
